{
	"name": "fun-comment",
	"displayName": "fun-comment",
	"description": "",
	"version": "0.0.1",
	"publisher":"zylab",
	"engines": {
		"vscode": "^1.27.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fun-comment.replaceText",
		"onCommand:fun-comment.chooseImage"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fun-comment.replaceText",
				"title": "Fun-comment Add Text Comment"
			},
			{
				"command": "fun-comment.chooseImage",
				"title": "Fun-comment Add Image Comment"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "fun-comment.replaceText",
					"group": "navigation"
				},
				{
					"when": "editorFocus",
					"command": "fun-comment.chooseImage",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "Fun Comment - Configuration",
			"properties": {
				"needWrap": {
					"type": "boolean",
					"default": true,
					"description": "If need wrap ascii with /**/"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.27.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"figlet": "^1.5.0",
		"get-pixels": "^3.3.2",
		"resize-image-buffer": "^1.0.0"
	}
}